<?xml version="1.0" encoding="UTF-8"?>
<core:deviceConfigs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:core="http://mobile.sensis.com.au/web/component/core/device"
    xmlns:map="http://mobile.sensis.com.au/web/component/map/device"
    xsi:schemaLocation="http://mobile.sensis.com.au/web/component/map/device au.com.sensis.mobile.web.component.map.device.xsd
        http://mobile.sensis.com.au/web/component/core/device au.com.sensis.mobile.web.component.core.device.xsd">
        
    <identifiedDeviceConfig>
        <deviceId>PC</deviceId>
        <deviceConfig xsi:type="map:DeviceConfig">
            <generateServerSideMap>false</generateServerSideMap>
        </deviceConfig>
    </identifiedDeviceConfig>    
        
    <identifiedDeviceConfig>
        <deviceId>Apple</deviceId>
        
        <deviceConfig xsi:type="map:DeviceConfig">
            <generateServerSideMap>false</generateServerSideMap>
        </deviceConfig>
        
        <userAgentDeviceConfig>
            <useragentRegex>.*Version/[012]\..*</useragentRegex>
            <deviceConfig xsi:type="map:DeviceConfig">
                <generateServerSideMap>true</generateServerSideMap>
            </deviceConfig>
        </userAgentDeviceConfig>
    </identifiedDeviceConfig>    
    
    <identifiedDeviceConfig>
        <deviceId>Android-OS</deviceId>
        <deviceConfig xsi:type="map:DeviceConfig">
            <generateServerSideMap>false</generateServerSideMap>
        </deviceConfig>
    </identifiedDeviceConfig>    
    
    <identifiedDeviceConfig>
        <deviceId>SAGEM-HomeManager</deviceId>
        <deviceConfig xsi:type="map:DeviceConfig">
            <generateServerSideMap>false</generateServerSideMap>
        </deviceConfig>
    </identifiedDeviceConfig>    
    
    <defaultDeviceConfig xsi:type="map:DeviceConfig">
        <generateServerSideMap>false</generateServerSideMap>
    </defaultDeviceConfig>
</core:deviceConfigs>
